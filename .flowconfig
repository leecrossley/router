[include]
.*/src/js*

[ignore]
.*/__tests__/.*
.*/www/*
.*/plugins/*
.*/platforms/*
.*/node_modules/react/.*
.*/node_modules/fbjs/.*
.*/node_modules/grunt-flow/.*
.*/node_modules/babel-*
.*/node_modules/babylon/.*

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
esproposal.class_instance_fields=enable
unsafe.enable_getters_and_setters=true
esproposal.class_static_fields=enable